import turtle
turtle.shapesize(0.001)
turtle.bgcolor("black")
turtle.pencolor("red")
turtle.penup()
turtle.goto(-50,-295)
turtle.pendown()
turtle.shape()
turtle.fillcolor("red")
turtle.begin_fill()
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)
turtle.forward(100)
turtle.right(90)
turtle.end_fill()
turtle.penup()
turtle.forward(50)
turtle.pendown()
turtle.left(90)
turtle.pencolor("orange")
turtle.fillcolor("orange")
turtle.speed(8)
turtle.speed(0)
turtle.shapesize(1)
turtle.shape("circle")
for i in range(5):
    turtle.right(1)
    turtle.forward(9)
    turtle.right(1)
    turtle.forward(9)
    turtle.right(1)
    turtle.forward(9)
    turtle.right(1)
