    turtle.forward(9)
    turtle.right(1)
    turtle.forward(9)
    turtle.right(1)
    turtle.forward(9)
    turtle.right(1)
    turtle.forward(9)
    turtle.right(1)
    turtle.forward(9)
    turtle.right(1)
